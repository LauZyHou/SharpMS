<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:s="clr-namespace:Plat._S;assembly=Plat"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Plat._V.DashCrossLine_V">
  <s:DashCrossLine Stroke="Black" StrokeThickness="2" StartPoint="{Binding Source.Pos, Mode=OneWay}"
		  EndPoint="{Binding Dest.Pos, Mode=OneWay}" IsHitTestVisible="False"/>
</UserControl>
